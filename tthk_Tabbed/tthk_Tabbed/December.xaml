<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="December"
             x:Class="tthk_Tabbed.December">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Christmas Eve"
                   x:Name="pyha1"
                   FontSize="Large"
                   Margin="6"
                   Padding="10"
                  />
            <Label Text="First day of Christmas"
                   x:Name="pyha2"
                   FontSize="Large"
                   Margin="6"
                   Padding="10"
                />
            <Label Text="Second day of Christmas"
                   x:Name="pyha3"
                   FontSize="Large"
                   Margin="6"
                   Padding="10"
                />
            <Image Source="present.png"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>