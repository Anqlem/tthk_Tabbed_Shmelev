<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="June"
             x:Class="tthk_Tabbed.June">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Victory Day"
                   x:Name="pyha1"
                   FontSize="Large"
                   Margin="6"
                   Padding="10"
                  />
            <Label Text="Midsummer day"
                   x:Name="pyha2"
                   FontSize="Large"
                   Margin="6"
                   Padding="10"
                />
            <Label Text="My birthday"
                   x:Name="pyha3"
                   FontSize="Large"
                   Margin="6"
                   Padding="10"
                />
            <Image Source="cap.png"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>